    "version": "0.2.0",
    "configurations": [
        {
            "name": "run dev",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "run staging",
            "request": "launch",
            "type": "dart",
            "args":[
                "--dart-define=ENVIRONMENT=STAGING"
            ]
        },
        {
            "name": "run production",
            "request": "launch",
            "type": "dart",
            "args":[
                "--dart-define=ENVIRONMENT=PROD"
            ]
        },
        {
            "name": "flutter_app_env_without_dot_env (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "flutter_app_env_without_dot_env (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}